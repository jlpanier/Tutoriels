<resources>
  <!-- https://docs.microsoft.com/en-us/xamarin/android/user-interface/material-theme -->
  
  <!-- colorPrimary – The color of the app bar. -->
  <color name="colorPrimary">@color/color800</color>

  <!-- colorPrimaryDark – The color of the status bar and contextual app bars; this is normally a dark version of colorPrimary.-->
  <color name="colorPrimaryDark">@color/color900</color>

  <!-- colorAccent – The color of UI controls such as check boxes, radio buttons, and edit text boxes.-->
  <color name="colorAccent">@color/color700</color>

  <!-- windowBackground – The color of the screen background.-->
  <color name="windowBackground">@color/color50</color>

  <!-- textColorPrimary – The color of UI text in the app bar.-->
  <color name="textColorPrimary">@color/color900</color>
  <color name="textColorSecondary">@color/color300</color>

  <!-- statusBarColor – The color of the status bar.-->
  <color name="statusBarColor">@color/color900</color>

  <!-- navigationBarColor – The color of the navigation bar.-->
  <color name="navigationBarColor">@color/color800</color>

  <color name="colorError">#B00020</color>

